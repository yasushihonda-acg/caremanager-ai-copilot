{
  "name": "caremanager-ai-copilot",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "dev:emulator": "firebase emulators:start",
    "dev:seed": "npx tsx scripts/seed.ts test-user-uid --emulator",
    "emulators": "firebase emulators:start",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:eval": "npx tsx tests/assessment/runEvaluation.ts",
    "test:eval:live": "npx tsx tests/assessment/runEvaluation.ts --live",
    "test:eval:help": "npx tsx tests/assessment/runEvaluation.ts --help",
    "test:live": "vitest run tests/assessment/extraction.live.test.ts --reporter=verbose"
  },
  "dependencies": {
    "firebase": "^10.14.0",
    "lucide-react": "^0.555.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.18",
    "@testing-library/react": "^16.3.2",
    "@types/node": "^22.14.0",
    "@vitejs/plugin-react": "^5.0.0",
    "@vitest/ui": "^4.0.18",
    "autoprefixer": "^10.4.23",
    "firebase-admin": "^13.6.1",
    "jsdom": "^28.1.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.18",
    "typescript": "~5.8.2",
    "vite": "^6.2.0",
    "vitest": "^4.0.18"
  }
}
